<?xml version="1.0" ?>
<sdf version="1.5">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>
    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane_grass</uri>
    </include>
    <!-- A dirt path -->
    <include>
      <uri>model://dirt_path_curves</uri>
	 <pose>0.0 0.0 0.001 0.0 0.0 0.0</pose>	<!-- Avoid z-fighting with ground plane -->
    </include>
  </world>
</sdf>
